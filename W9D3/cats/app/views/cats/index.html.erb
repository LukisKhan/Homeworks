<h1>Cats#index</h1>
<p>Find me in app/views/cats/index.html.erb</p>

<ul>
    <% @cats.each do |cat| %>
    <li>
        <a href="<%= cat_url(cat.id) %>"> <%= cat.name %></a>
        <%= cat.color %>
        <%= cat.sex %>
        <%= cat.description %>
        <%= cat.birth_date %>
    </li>   
    <% end %>
</ul>